// -*- go -*-

package main

import (
	"crypto/rsa"
)

type Size = uint32

type Garbler struct {
	msg       Size
	privShare Size
	pubN      Size
	pubE      Size
}

func main(g Garbler, privShare Size) (uint, uint) {

	priv := g.privShare + privShare

	cipher := rsa.Encrypt(g.msg, g.pubE, g.pubN)
	plain := rsa.Decrypt(cipher, priv, g.pubN)

	return cipher, plain
}
