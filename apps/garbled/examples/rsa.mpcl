//

package main

import (
	"crypto/rsa"
)

// d: 0x321af139
// n: 0xd60b2b09
// e: 0x10001
//
// private: d, e
// public:  n, e

// msg: 0x6d7472
// ciper: 0x61f9ef88

func main(mode bool, msg, gD, pubN, pubE, eD uint64) uint64 {
	var d uint64 = 0

	d = gD + eD

	if mode {
		return rsa.Encrypt(msg, pubE, pubN)
	} else {
		return rsa.Decrypt(msg, d, pubN)
	}
}
