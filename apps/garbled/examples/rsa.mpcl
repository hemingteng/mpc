package main

import (
	"crypto/rsa"
)

// d: 0x321af139
// n: 0xd60b2b09
// e: 0x10001
//
// private: d, e
// public:  n, e

// msg: 0x6d7472
// ciper: 0x61f9ef88

func main(msg, gD, pubN, pubE, eD uint32) (uint32, uint32) {
	var d uint32 = 0

	d = gD + eD

	cipher := rsa.Encrypt(msg, pubE, pubN)
	plain := rsa.Decrypt(cipher, d, pubN)

	return cipher, plain
}
